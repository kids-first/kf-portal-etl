{
  "mappings": {
      "properties": {
        "kf_id": {
          "type": "keyword"
        },
        "name": {
          "type": "keyword"
        },
        "search": {
          "type": "text",
          "fields": {
            "raw": {
              "type": "keyword"
            }
          },
          "analyzer": "autocomplete"
        },
        "external_id": {
          "type": "keyword"
        },
        "data_access_authority": {
          "type": "keyword"
        },
        "code": {
          "type": "keyword"
        },
        "domain": {
          "type": "keyword"
        },
        "program": {
          "type": "keyword"
        },
        "participant_count": {
          "type": "long"
        },
        "file_count": {
          "type": "long"
        },
        "family_count": {
          "type": "long"
        },
        "family_data": {
          "type": "boolean"
        },
        "experimental_strategy": {
          "type": "keyword"
        },
        "data_categories": {
          "type": "keyword"
        },
        "data_category_count": {
          "properties": {
            "data_category": {
              "type": "keyword"
            },
            "count": {
              "type": "long"
            }
          },
          "type": "nested"
        }
      }

  },
  "settings": {
    "analysis": {
      "analyzer": {
        "autocomplete": {
          "filter": [
            "lowercase",
            "edge_ngram",
            "studies_ascii_folding"
          ],
          "type": "custom",
          "tokenizer": "standard"
        }
      },
      "filter": {
        "edge_ngram": {
          "max_gram": "20",
          "min_gram": "1",
          "side": "front",
          "type": "edge_ngram"
        },
        "studies_ascii_folding": {
          "type": "asciifolding",
          "preserve_original": "true"
        }
      }
    },
    "index.mapping.nested_fields.limit": 100,
    "index.max_result_window": 100000000
  }
}
